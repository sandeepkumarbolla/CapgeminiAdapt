<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
   
   <bean  id = "Question1" class="io.springcore.q2.Question">
  <property name="questionId" value="1"></property>
  <property name="question" value= "Where is Amsterdam?"></property> 
  <property name="answer" value="Netherlands"></property>
  
   </bean>
   <bean id="Question2" class="io.springcore.q2.Question">
    <property name="questionId" value="2"></property>
  <property name="question" value= "Where is Frankfurt?"></property> 
  <property name="answer" value="Germany"></property>
   </bean>
   
   
   
   
   
   
   
   
   </beans>